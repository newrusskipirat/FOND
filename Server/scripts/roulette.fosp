		case(1):
		multipl=(18);
		break;
		case(2):
		multipl=(12);
		break;
		case(3):
		multipl=(9);
		break;
		case(4):
		multipl=(6);
		break;
		case(5):
		case(6):
		multipl=(3);
		break;
		case(7):
		case(8):
		case(9):
		multipl=(2);
		break;
	}
	return multipl;
}

