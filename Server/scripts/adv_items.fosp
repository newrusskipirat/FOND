uint e_FireBurn(uint[]@values)
{
	Item@item=GetItem(values[0]);
	if(item.Val2>0){
		values[1]++;
		if(values[1]>=60){values[1]=0;item.Val2--;}
		if(Random(0,1)==0){
			Map@map=GetMap(item.MapId);
			if((@map!=null))map.SetText(item.HexX,item.HexY,((uint((0xFF<<24)|(((0x7F)&0xFF)<<16)|(((0x7F)&0xFF)<<8)|((0x7F)&0xFF)))),":огонь горит:");
		}
		return((Random(4,10))*__TimeMultiplier);
	}
	LightControl(item,false,0,0);
	if(item.Val5==(3))
	{
		DeleteItem(item);
		return 0;
	}
	ChangeFrame(item,0);
	item.Val0=1;
	EraseTimeEvent(item.Val3);
	item.Val3=0;
	return 0;
}

void ChangeFrame(Item&item,uint8 frame)
{
	item.AnimStayBegin=frame;
	item.AnimStayEnd=frame;
	item.AnimShowBegin=frame;
	item.AnimShowEnd=frame;
	item.AnimHideBegin=frame;
	item.AnimHideEnd=frame;
	item.Update();
	return;
}

