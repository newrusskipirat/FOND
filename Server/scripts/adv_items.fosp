int[][]groups={{},{7907},{7900},{7901,286},{7902,7903,7904},{7905,7906}};
string[]group_descriptions={"ERROR","генераторе","бочке","костре","колодце","кактусе"};      

int FindGroup(Item&item){
	int PID=item.GetProtoId();
	for(uint i=0,iEnd=groups.length();i<iEnd;i++)
	if(groups[i].find(PID)!=-1)return i;
	return(0);
}        

void _InitItem(Item&item,bool firstTime)
{
	int key=FindGroup(item);
	if(key>(0))
	{
		ChangeFrame(item,0);
		item.Val0=0;
		if(key==(4)||key==(5)){
			item.Val1=(key==(5)?Random(10,15):Random(25,75));
			item.Val2=item.Val1;
		}
		else
		{
			item.Val1=10;
			item.Val2=0;
		}
		item.Val3=0;
		if(key==(4)){
			uint[]values={item.Id};
			item.Val3=CreateTimeEvent(__FullSecond+((Random(15,45))*__TimeMultiplier*60),"e_WellCharge",values,false);
		}
		item.Val4=0;
		item.Val5=key;
		item.SetEvent((4),"e_Skill");
		item.SetEvent((3),key==(5)?"e_Cut":"e_Repair");
	}
}

bool e_Skill(Item&item,Critter&cr,int skill)
{
	if(item.Val0!=0){
		cr.Say((11),"Предмет требует починки!");
		return true;
	}
	string description=group_descriptions[item.Val5];
	switch(skill)
	{
		case(212):
		cr.Say((11),"В "+description+" осталось "+item.Val2+".");
		return true;
		case(-1):
		switch(item.Val5){
			case((2)):
			case((3)):
			if(item.Val2>0)return ShowMenu(cr);
			else{cr.Say((11),"Огонь погас, используйте древесину и зажигалку.");item.Val0=1;}
			return true;
			
			case((4)):
			if(item.Val2>0)return ShowMenuWater(cr,item);
			else{
				cr.Say((11),"В "+description+" почти нет воды. Подождите часок-другой или поработайте лопатой.");
				item.Val0=3;
			}
			return true;
			default:break;
		}
	}
	return false;
}

bool ShowMenuWater(Critter&cr,Item&item){
	if(item.Val2<=0){cr.Say((11),"В колодце кончилась вода. Подождите часок-другой или поработайте лопатой.");item.Val0=3;return true;}
	cr.ShowScreen((2),1,"adv_items@answer_WATER");
	cr.StatBase[(97)]=item.Id;
	cr.Say((18),"В "+group_descriptions[item.Val5]+" осталось "+item.Val2+".");
	cr.Say((19+(0)),"набрать воды");
	return true;
}

int[]BottlePIDs_before={(542),(532)};
int[]BottlePIDs_after={(1627),(533)};
void answer_WATER(Critter&cr,uint answerI,string&answerS)
{
	if(answerI==0){
		Item@hand=cr.GetItem(0,(1));
		if(!(@hand!=null)){cr.Say((11),"Возьмите пустую тару в руки!");return;}
		int n=BottlePIDs_before.find(hand.GetProtoId());
		if(n==-1){cr.Say((11),"Эта тара не подходит!");return;}
		if(cr.Timeout[(232)]>0){
			cr.Say((11),"Вы слишком устали, дайте отдых рукам.");return;
		}
		Map@map=cr.GetMap();
		if(!(@map!=null)){Log("Вызов с глобала!");return;}
		Item@source=map.GetItem(cr.StatBase[(97)]);
		if(!(@source!=null)){Log("Потеря указателя на предмет!");return;}             
		
		source.Val2--;
		do{if(hand.GetCount()>(1))
			hand.SetCount(hand.GetCount()-(1));else
			DeleteItem(hand);}while(false);
		cr.AddItem(BottlePIDs_after[n],1);
		cr.Say((5),"набирает воду");
		cr.TimeoutBase[(232)]=__FullSecond+5*((3)*__TimeMultiplier);
		cr.AddTimeEvent("main@cte_Tired",5*((3)*__TimeMultiplier),(14),0);
		ShowMenuWater(cr,source);
	}
}  

int[]CactusCutting_tools={(4),(543),(236),(45),(517),
	(1418),(1419),(383),(319),(522)};
bool e_Cut(Item&item,Critter&cr,Item@usedItem)
{
	if(item.Val2==0){
		cr.Say((11),"Этот кактус полностью выжат, ему потребуется около получаса на восстановление.");
		uint[]values={item.Id};
		item.Val3=CreateTimeEvent(__FullSecond+((Random(15,45))*__TimeMultiplier*60),"e_CactusCharge",values,false);
		return true;
	}
	if(cr.Timeout[(232)]>0){
		cr.Say((11),"Вы слишком устали, дайте отдых рукам.");return true;
	}
	if(!(@usedItem!=null)||CactusCutting_tools.find(usedItem.GetProtoId())==-1){
		cr.Say((11),"Что бы извлечь из кактуса влагу, вам потребуется использовать любой инструмент из списка:");
		for(uint i=0,iEnd=CactusCutting_tools.length();i<iEnd;i++)
		cr.SayMsg((11),(2),CactusCutting_tools[i]*100);
		return true;
	}
	Item@hand=cr.GetItem(0,(1));
	if(!(@hand!=null)){cr.Say((11),"Возьмите пустую тару в руки!");return true;}
	int n=BottlePIDs_before.find(hand.GetProtoId());
	if(n==-1){cr.Say((11),"Эта тара не подходит!");return true;}
	do{if(hand.GetCount()>(1))
		hand.SetCount(hand.GetCount()-(1));else
		DeleteItem(hand);}while(false);
	cr.AddItem(BottlePIDs_after[n],1);
	cr.TimeoutBase[(232)]=__FullSecond+1*((30)*__TimeMultiplier);
	cr.AddTimeEvent("main@cte_Tired",1*((30)*__TimeMultiplier),(14),0);
	cr.Say((5),"набирает воду");
	return false;
}    

int[][]Repairing_resources={{(127),(50),(286)},{(286)}};
int[][]Repairing_tools={{(75),(308),(289)},{(101)}};
int[][]Repairing_iterations={{1,2,3},{0}};

bool e_Repair(Item&item,Critter&cr,Item@usedItem)
{
	if(item.Val0==0&&!(@usedItem!=null))return e_Skill(item,cr,(-1));
	if(cr.Timeout[(232)]>0){
		cr.Say((11),"Вы слишком устали, дайте отдых рукам.");return true;
	}
	int n=-1;
	uint[]values={item.Id,0};
	bool initFire=false;
	switch(item.Val5){
		case((0)):
		if(item.GetProtoId()==(286)){
			do{if(usedItem.GetCount()>(1))
				usedItem.SetCount(usedItem.GetCount()-(1));else
				DeleteItem(usedItem);}while(false);
			item.Val2=2;
			item.Val5=(3);
			item.Val3=CreateTimeEvent(__FullSecond+((Random(4,10))*__TimeMultiplier),"e_FireBurn",values,false);
			LightControl(item,true,0,0);
			item.ChangeProto((7901));
			item.SetEvent((4),"e_Skill");
			item.SetEvent((3),"e_Repair");
			item.SetEvent((7),"e_Hot");
			item.Update();
			return true;
		}break;
		case((4)):n=0;break;
		case((2)):
		case((3)):n=1;initFire=true;break;
		default:break;
	}
	if(n==-1)return false;
	if(initFire&&(@usedItem!=null)){
		if(usedItem.GetProtoId()==(286)&&item.Val0==0){
			if(item.Val2<item.Val1){
				cr.Say((5),"подкладывает дрова");
				do{if(usedItem.GetCount()>(1))
					usedItem.SetCount(usedItem.GetCount()-(1));else
					DeleteItem(usedItem);}while(false);
				item.Val2++;
			}else cr.Say((11),"Дрова уже некуда класть, дайте этим прогореть!");
			return true;
		}
		if(usedItem.GetProtoId()==(20)){
			int count=item.Val2*8/10;
			if(count>0)cr.AddItem((286),count);
			cr.Say((5),"выгребает древесину");
			item.Val2=0;
			item.Val3=CreateTimeEvent(0,"e_FireBurn",values,false);
			return true;
		}
		int pid=BottlePIDs_after.find(usedItem.GetProtoId());
		if(pid!=-1){
			cr.Say((5),"тушит огонь");
			do{if(usedItem.GetCount()>(1))
				usedItem.SetCount(usedItem.GetCount()-(1));else
				DeleteItem(usedItem);}while(false);
			cr.AddItem(BottlePIDs_before[pid],1);
			EraseTimeEvent(item.Val3);
			item.Val3=0;
			item.Val0=1;
			Map@map=GetMap(item.MapId);
			if((@map!=null))map.SetText(item.HexX,item.HexY,((uint((0xFF<<24)|(((0x7F)&0xFF)<<16)|(((0x7F)&0xFF)<<8)|((0x7F)&0xFF)))),":шипение пара:");
			return true;
		}
	}
	Item@hand=cr.GetItem(0,(1));
	if(!(@usedItem!=null)||usedItem.GetProtoId()!=Repairing_resources[n][item.Val0-1])return NeededItem(cr,Repairing_resources[n][item.Val0-1]);
	if(!(@hand!=null)){cr.Say((11),"Что бы приступить к ремонту, возьмите инструмент в руки!");
		return NeededItem(cr,Repairing_tools[n][item.Val0-1]);}
	if(hand.GetProtoId()!=Repairing_tools[n][item.Val0-1])return NeededItem(cr,Repairing_tools[n][item.Val0-1]);
	cr.Say((5),initFire?"разжигает огонь":"ремонтирует");
	cr.TimeoutBase[(232)]=__FullSecond+(11-cr.Stat[(2)])*((30)*__TimeMultiplier);
	if(item.Val4<Repairing_iterations[n][item.Val0-1]){
		item.Val4++;
		cr.Say((11),"Осталось ещё "+(Repairing_iterations[n][item.Val0-1]-item.Val4+1)+" итераций.");
		return true;
	}else{
		item.Val2+=initFire?1:Repairing_iterations[n][item.Val0-1]*3;
		item.Val0=0;
		item.Val4=0;
		do{if(usedItem.GetCount()>(1))
			usedItem.SetCount(usedItem.GetCount()-(1));else
			DeleteItem(usedItem);}while(false);
		if(initFire){
			item.Val3=CreateTimeEvent(((Random(4,10))*__TimeMultiplier),"e_FireBurn",values,false);
			LightControl(item,true,0,0);
		}
		return e_Skill(item,cr,(-1));
	}
}

