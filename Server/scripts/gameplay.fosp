}

uint cte_IncreaseStudyExp(Critter&cr,int identifier,uint&rate)
{
	if(cr.IsDead()||cr.Quest[(703)]==(0))return 5*60*__TimeMultiplier; 
	
	uint16 studyExp=cr.StatBase[(138)];
	uint16 studyExpMax=cr.StatBase[(139)];
	
	if(cr.Quest[(703)]!=(1))return 5*60*__TimeMultiplier;
	
	if(cr.Stat[(77)]>=12){
		if(studyExpMax>studyExp){
			uint16 studyTemp=studyExpMax*5/30;
			studyTemp=(((studyExp+studyTemp)>(studyExpMax>1000?studyExpMax:1000))?(studyExpMax>1000?studyExpMax:1000):(((studyExp+studyTemp)<(0))?(0):(studyExp+studyTemp)));
			cr.StatBase[(138)]=studyTemp;
		}else{
			if(cr.ParamBase[(856)]==0)
			cr.Say((11),"Вас переполняют знания, которыми вы хотите поделиться..");
		}
	}
	
	return 5*60*__TimeMultiplier;
}

import void AffectPoison(Critter&cr,int value)from"poison";

uint cte_Hunger(Critter&cr,int identifier,uint&rate)
{
	if(cr.IsDead()||cr.Quest[(703)]==(0))return((Random(20,40)+cr.Stat[(2)]*5)/(1+cr.Trait[(550)]))*5*__TimeMultiplier;
	if(cr.IsKnockout()&&cr.GetTimeEvents((51),null,null,null)==0)return((Random(20,40)+cr.Stat[(2)]*5)/(1+cr.Trait[(550)]))*5*__TimeMultiplier;
	
	string[]feelings={"легкую усталость","прилив сил","успокоение","свежесть","что-то","себя лучше","себя хуже"};
	string feeling="";
	
	cr.StatBase[(128)]-=Random(0,1)==0?1:0;
	if(cr.StatBase[(128)]>0){
		if(cr.StatBase[(128)]<25){
			feeling+=(feeling==""?"":", ")+"сильный голод";
		}
		else if(cr.StatBase[(128)]<50){
			feeling+=(feeling==""?"":", ")+"голод";
		}
		else if(cr.StatBase[(128)]<75){
			feeling+=(feeling==""?"":", ")+"легкий голод";
		}
	}
	else{
		feeling+=(feeling==""?"":", ")+"нестерпимый голод";
	}
	
	cr.StatBase[(127)]-=Random(0,1)==0?1:0;
	if(cr.StatBase[(127)]>0){
		if(cr.StatBase[(127)]<25){
			feeling+=(feeling==""?"":", ")+"сильную жажду";
		}
		else if(cr.StatBase[(127)]<50){
			feeling+=(feeling==""?"":", ")+"жажду";
		}
		else if(cr.StatBase[(127)]<75){
			feeling+=(feeling==""?"":", ")+"легкую жажду";
		}
	}
	else{
		feeling+=(feeling==""?"":", ")+"невыносимую жажду";
	}
	
	if(cr.StatBase[(148)]>0)
	{
		cr.StatBase[(148)]-=1;
		int DoseTreshold=(cr.Stat[(2)]+cr.Stat[(0)])
		*(1+cr.Trait[(562)]);
		if(cr.StatBase[(148)]<=DoseTreshold/4)
		feeling+=(feeling==""?"":", ")+"усталость";
		else if(cr.StatBase[(148)]<=DoseTreshold/3)
		feeling+=(feeling==""?"":", ")+"слабость";
		else if(cr.StatBase[(148)]<=DoseTreshold/2)
		feeling+=(feeling==""?"":", ")+"ломоту в теле";
		else if(cr.StatBase[(148)]<=DoseTreshold*3/4)
		feeling+=(feeling==""?"":", ")+"боль в груди";
		else if(cr.StatBase[(148)]<=DoseTreshold)
		feeling+=(feeling==""?"":", ")+"сильную головную боль";
	}
	
	uint8 toxicLevel=cr.StatBase[(147)];
	if(toxicLevel>=cr.Stat[(2)]*10){
		cr.ToDead(Random(0,1)==0?(102):(103),null); 
		
		cr.Say((11),"Вы умерли от заражения крови.");
		return((Random(20,40)+cr.Stat[(2)]*5)/(1+cr.Trait[(550)]))*5*__TimeMultiplier;
	}else if(toxicLevel>=cr.Stat[(2)]*8){
		feeling+=(feeling==""?"":", ")+"себя ужасно, вас лихорадит";
	}else if(toxicLevel>=cr.Stat[(2)]*5){
		feeling+=(feeling==""?"":", ")+"себя плохо, вам тяжело дышать";
	}else if(toxicLevel>=cr.Stat[(2)]*2){
		feeling+=(feeling==""?"":", ")+"жар";
	}else if(toxicLevel>0){
		feeling+=(feeling==""?"":", ")+"затруднённое дыхание";
	}
	
	if(cr.StatBase[(147)]>=10){
		cr.StatBase[(147)]+=cr.StatBase[(147)]/10;
		AffectPoison(cr,Random(toxicLevel/10,toxicLevel/5));
	}
	
	if(feeling=="")feeling=feelings[Random(0,feelings.length()-1)];
	
	if(cr.ParamBase[(856)]==0)
	cr.Say((11),"Вы чувствуете "+feeling+".");
	
	return((Random(20,40)+cr.Stat[(2)]*5)/(1+cr.Trait[(550)]))*5*__TimeMultiplier;
}

uint cte_Exp(Critter&cr,int identifier,uint&rate)
{
	if(cr.Quest[(703)]==(3)||cr.Quest[(703)]==(0))return 15*60*__TimeMultiplier;
	
	if(!cr.IsKnockout())
	{      
		
		cr.StatBase[(76)]+=250*(1+cr.AddictionBase[(475)]);
	}
	return 15*60*__TimeMultiplier;
}

