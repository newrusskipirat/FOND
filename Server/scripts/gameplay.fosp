	bool global_critter_init(Critter&cr,bool firstTime)
	{
		if(cr.IsNpc())
		return true;
		
		if(firstTime)
		{
			cr.QuestBase[(703)]=(-1);
			
			for(uint y=0;y<__GlobalMapHeight;y++)
			for(uint x=0;x<__GlobalMapWidth;x++)
			cr.SetFog(x,y,(3));           
			
		}
		
		if(cr.Quest[(703)]==(-1))
		{
			if(TeleportToStart(cr))
			cr.QuestBase[(703)]=(0);
			else
			{
				cr.Disconnect();
				return false;
			}
		}
		
		Map@map=cr.GetMap();
		
		if(!IsInGame(cr))
		{
			if(cr.Quest[(703)]>(0)&&(map is null))
			{
				CreateModeStart(cr);
			}
		}
		else
		{
			if(cr.Quest[(703)]==(3))
			{
				DontPickItUp(cr);
				cr.ToDead(0,null);
			}
		}
		
		AfterLogin(cr);
		
		return true;
	}
