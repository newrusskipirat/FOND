	}
	string@[]@layer=split(word2,"!");
	uint8 length=layer.length();  
	
	for(uint8 i=0;i<length;i++)
	{
		
		string@[]@layerData=split(layer[i],"*");      
		
		if(layerData.length()>1)
		{
