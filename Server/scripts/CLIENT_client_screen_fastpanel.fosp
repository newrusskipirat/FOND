void LoadPanel(int p0,int p1,int p2,string@word2,int[]@p4)
{
	uint8 count=1;
	uint8 count1=0;
	uint color=0;     
	
	IGUIElementOpt@fastBtn;
	
	if(p0!=0)
	{
		word2=__tempFastText;
	}
