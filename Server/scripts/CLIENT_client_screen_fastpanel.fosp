				Message("button is null");
				break;
			}
			
			fastBtn.SetElementFastPanelType(type);
			fastBtn.SetElementFastPanelId(id);
			fastBtn.Text(text,(5),color,((uint((0xFF<<24)|(((0)&0xFF)<<16)|(((0xC8)&0xFF)<<8)|((0)&0xFF)))),(0x0004)|(0x0008));
			fastBtn.SetDescription(desc);
			GetPic(fastBtn);      
			
		}
	} 
	
}

