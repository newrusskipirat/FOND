uint LoadPanelTempFile(bool type)
{
	file f;
	string word="",word2="";
	uint pos=0;
	
	if(f.open("FastPanel.cfg","r")==0)
	{
		
		f.setPos(0);
		while(!f.isEndOfFile())
		{
			pos=f.getPos();
			f.readLine(word);
			word2+=word; 
			
		}
		__tempFastText=word2;
		f.close();
	}
	
	if(pos!=0&&type)
	return pos;
	return 0;
}

