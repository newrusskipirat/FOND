{
	switch(fastBtn.GetFastPanelType())
	{
		case 1:
		fastBtn.UpPic("skilln.frm");
		fastBtn.DownPic("skillh.frm");
		break;
		case 2:
		if(fastBtn.GetFastPanelId()!=5)
		{
			fastBtn.UpPic("talkn.frm");
			fastBtn.DownPic("talkh.frm");
		}
		else
		{
