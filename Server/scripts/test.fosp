void SetRain(Critter&player,int power,int time,int cooldown)
{
	Map@map=player.GetMap();
	if(!(@map!=null))
	{
		player.Say((11),"На глобале дождь не скастовать.");
		return;
	}
	
	GameVar@rain=GetLocalVar((9991),map.Id);
	if(!(@rain!=null))return;
	
	if(rain.GetValue()>0)
	EraseTimeEvent(rain.GetValue()); 
	
	if(power==0||time==0||cooldown==0)
	{
		player.Say((11),"Пиратский дождь вызывается ONLY на этой карте!\nАргументы вызова пиратского дождя: мощность, время длительности дождя, таймаут между дождями.");
		return;
	}
	
	if(power<0||time<0||cooldown<0)
	{
		player.Say((11),"Пиратский дождь отключен.");
		EraseTimeEvent(rain.GetValue());
		return;
	} 
	
	uint[]values={power,time,cooldown,map.Id,1};
	rain.opAssign(CreateTimeEvent(__FullSecond,"e_piratRain",values,false));
	
}

uint e_piratRain(uint[]@values)
{
	if(!(@values!=null)||values.length()!=5)return 0;
	Log(values[0]+" "+values[1]+" "+values[2]+" "+values[3]+" "+values[4]);
	Map@map=GetMap(values[3]);
	if(!(@map!=null))return 0;
	
	if(values[4]==1)
	{
		if(Random(1,4)==1)
		{map.SetRain(values[0]);}
		else{map.SetRain(0);}
		values[4]=0;
		
		return((values[1])*__TimeMultiplier);
	}
	else
	{
		map.SetRain(0);
		values[4]=1;
		
		return((values[2])*__TimeMultiplier);
	}
}