	for(uint i=0;i<count;i++)
	{
		Item@item=items[i];
		bool canPickUp=false;
		switch(item.GetType())
		{
			case(1):
			case(2):
			case(3):
			case(4):
			case(5):
			case(7):
			canPickUp=true;
			break;
			
			case(8):
			canPickUp=(((item.Flags)&((0x08000000)))!=0);
			break;
			case(9):
			(item.Flags=(item.Flags)|((0x00001000)));
			break;
			default:
			break;
