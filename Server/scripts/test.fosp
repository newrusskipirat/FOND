			furnace.Val1=0;
			return((30)*__TimeMultiplier);
		}
		if(temp==0){
			map.SetText(furnace.HexX,furnace.HexY,((uint((0xFF<<24)|(((0x7F)&0xFF)<<16)|(((0x7F)&0xFF)<<8)|((0x7F)&0xFF)))),":печь остыла:");
			return 0;
		}
	}
	else
	{
		if(temp==0){
			map.SetText(furnace.HexX,furnace.HexY,((uint((0xFF<<24)|(((0x7F)&0xFF)<<16)|(((0x7F)&0xFF)<<8)|((0x7F)&0xFF)))),":огонь разгорается в печи:");
			furnace.Val1=1;
			return((30)*__TimeMultiplier);
		}
		if(temp==1){
			map.SetText(furnace.HexX,furnace.HexY,((uint((0xFF<<24)|(((0x7F)&0xFF)<<16)|(((0x7F)&0xFF)<<8)|((0x7F)&0xFF)))),":печь нагревается:");
			furnace.Val1=2;
			return((30)*__TimeMultiplier);
		}
		if(temp==2){
			string[]desc={"громкий треск огня в печи","от печи исходит жар","мерцание пламени в печи","из печи валит дым"};
			map.SetText(furnace.HexX,furnace.HexY,((uint((0xFF<<24)|(((0x7F)&0xFF)<<16)|(((0x7F)&0xFF)<<8)|((0x7F)&0xFF)))),":"+desc[Random(0,desc.length()-1)]+":");
			
			Item@[]resources;
			int count=furnace.GetItems(0,resources);
			if(count>0)
			{
				int n=Random(0,count-1);
				for(uint i=0;i<FurnaceResourceTransfer.length();i++)
				{
					if((@FurnaceResourceTransfer[i]!=null)&&FurnaceResourceTransfer[i].length()==4
					&&resources[n].GetProtoId()==FurnaceResourceTransfer[i][0]
					&&resources[n].GetCount()>=FurnaceResourceTransfer[i][1])
