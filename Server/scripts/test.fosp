bool _FurnaceOnMe(Item&item,Critter&cr,Item@usedItem)
{
	
	Item@wood;
	uint[]values={item.Id};
	
	switch(usedItem.GetProtoId())
	{
		case((533)):
		if(item.Val0==0)return false;
		cr.Say((6),"тушит огонь водой");
		do{if(usedItem.GetCount()>(1))
			usedItem.SetCount(usedItem.GetCount()-(1));else
			DeleteItem(usedItem);}while(false);
		cr.AddItem((532),1);
		item.Val0=0;
		break;
		case((1627)):
		if(item.Val0==0)return false;
		cr.Say((6),"тушит огонь водой");
		do{if(usedItem.GetCount()>(1))
			usedItem.SetCount(usedItem.GetCount()-(1));else
			DeleteItem(usedItem);}while(false);
		cr.AddItem((542),1);
		item.Val0=0;
		break;
		case((101)):
		if(item.Val0==1)return false;
		@wood=item.GetItem((286),0);
		if(!(@wood!=null))
		{
			cr.Say((11),"В печке нет дров для розжига.");
			return false;
		}
		cr.Say((6),"разжигает огонь в печи");
		item.Val0=1;
		EraseTimeEvent(item.Val2);
		item.Val2=CreateTimeEvent(__FullSecond,"e_FurnaceBurn",values,false);
		break;
		case((289)):
		cr.Say((6),"орудует совком");
		cr.ShowContainer(null,item,(2));
		break;
		case((20)):
		cr.Say((6),"выгребает дрова");
		@wood=item.GetItem((286),0);
		if(!(@wood!=null))return false;
		MoveItem(wood,wood.GetCount(),cr.GetMap(),cr.HexX,cr.HexY);
		break;
		default:
		cr.Say((6),"бросает что-то в печь");
		MoveItem(usedItem,1,item,0);
		break;
	}
	return true;
}

